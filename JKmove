    -------------------< movement >---------------------
        --// sorry...
        task.spawn(function()
        task.wait(0.2)
        local TweenService = game:GetService("TweenService")
        local initial_position = humanoidrootpart.CFrame
        local tweenInfo = TweenInfo.new(5, Enum.EasingStyle.Quad, Enum.EasingDirection.In)
        local tween = TweenService:Create(humanoidrootpart, tweenInfo, {CFrame = initial_position + Vector3.new(0, 0, 0)})
        tween:Play()
        tween.Completed:Wait()
        tweenInfo = TweenInfo.new(25, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
        tween = TweenService:Create(humanoidrootpart, tweenInfo, {CFrame = initial_position + Vector3.new(0, 0, 0)})
        tween:Play()
        tween.Completed:Wait()
        tweenInfo = TweenInfo.new(10, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
        tween = TweenService:Create(humanoidrootpart, tweenInfo, {CFrame = initial_position + Vector3.new(0, 0, 0)})
        tween:Play()
        tween.Completed:Wait()
        tweenInfo = TweenInfo.new(0.6, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
        tween = TweenService:Create(humanoidrootpart, tweenInfo, {CFrame = initial_position + Vector3.new(0, 0, 0)})
        tween:Play()
        tween.Completed:Wait()
        task.wait(0.75)
        flute_track:Stop(); hold_track:Stop();
        freezeVelo = false
        end)
    ----------------------------------------------------
    ----------------< rotation >-----------------
        task.spawn(function()
        game:GetService("RunService").RenderStepped:Connect(function()
            if not freezeVelo then
                return
            end
            humanoidrootpart.Velocity = Vector3.zero
        end)
        local TweenService = game:GetService("TweenService")
        local tweenInfo = TweenInfo.new(0.18, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut)
        local tween = TweenService:Create(humanoidrootpart, tweenInfo, {CFrame = humanoidrootpart.CFrame * CFrame.Angles(math.rad(-0), 0, 0)})
        tween:Play()
        tween.Completed:Wait()
        task.wait(0.2)
        humanoidrootpart.CFrame = humanoidrootpart.CFrame * CFrame.Angles(math.rad(-0), 0, 0)
        tweenInfo = TweenInfo.new(15, Enum.EasingStyle.Sine, Enum.EasingDirection.Out)
        tween = TweenService:Create(humanoidrootpart, tweenInfo, {CFrame = humanoidrootpart.CFrame * CFrame.Angles(math.rad(-0), 20, 0)})
        tween:Play()
        tween.Completed:Wait()
        end)
    ---------------------------------------------
    ---------------< general >-------------
        task.spawn(function()
        for _, track in pairs(humanoid:GetPlayingAnimationTracks()) do
            track:Stop()
        end
        local org_WalkSpeed = humanoid.WalkSpeed
        local org_AutoRotate = humanoid.AutoRotate
        local org_JumpPower = humanoid.JumpPower
        humanoid.WalkSpeed = 0
        humanoid.AutoRotate = false
        humanoid.JumpPower = 0
        task.wait(3)
        humanoid.WalkSpeed = org_WalkSpeed
        humanoid.AutoRotate = org_AutoRotate
        humanoid.JumpPower = org_JumpPower
        humanoid.HipHeight = 0
        active_move = nil
        end)
    ---------------------------------------
end

local flex_works_skill = Instance.new("Tool")
flex_works_skill.Name = "Unlimited Flex Works"
flex_works_skill.RequiresHandle = false
flex_works_skill.Parent = player.Backpack
flex_works_skill.Equipped:Connect(function()
    flex_works()
    task.wait()
    flex_works_skill.Parent = player.Backpack
end)
local function hTwenty_SERIES()
    if active_move then
        return
    end
    active_move = "honored one"
    local flute_track
    local hold_track
    local freezeVelo = true
    -------------------< animations >-------------------
        task.spawn(function()
        task.wait(0)
        local flute_anim = Instance.new("Animation")
        flute_anim.AnimationId = "rbxassetid://18897538537"
        local hold_anim = Instance.new("Animation")
        hold_anim.AnimationId = "rbxassetid://18897538537"
        flute_track = humanoid:LoadAnimation(flute_anim)
        hold_track = humanoid:LoadAnimation(hold_anim)
        flute_track:Play(); hold_track:Play();
        task.wait(100)
        flute_track:AdjustSpeed(0); flute_track:AdjustSpeed(0);
	end)
		
    ----------------------------------------------------
    -------------------< movement >---------------------
        --// sorry...
        task.spawn(function()
        task.wait(0.2)
        local TweenService = game:GetService("TweenService")
        local initial_position = humanoidrootpart.CFrame
        local tweenInfo = TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.In)
        local tween = TweenService:Create(humanoidrootpart, tweenInfo, {CFrame = initial_position + Vector3.new(0, 0, 0)})
        tween:Play()
        tween.Completed:Wait()
        tweenInfo = TweenInfo.new(0.75, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
        tween = TweenService:Create(humanoidrootpart, tweenInfo, {CFrame = initial_position + Vector3.new(0, 0, 0)})
        tween:Play()
        tween.Completed:Wait()
        tweenInfo = TweenInfo.new(0.6, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
        tween = TweenService:Create(humanoidrootpart, tweenInfo, {CFrame = initial_position + Vector3.new(0, 0, 0)})
        tween:Play()
        tween.Completed:Wait()
        tweenInfo = TweenInfo.new(0.6, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
        tween = TweenService:Create(humanoidrootpart, tweenInfo, {CFrame = initial_position + Vector3.new(0, 0, 0)})
        tween:Play()
        tween.Completed:Wait()
        task.wait(0.75)
        flute_track:Stop(); hold_track:Stop();
        freezeVelo = false
        end)
    ----------------------------------------------------
    ----------------< rotation >-----------------
        task.spawn(function()
        game:GetService("RunService").RenderStepped:Connect(function()
            if not freezeVelo then
                return
            end
            humanoidrootpart.Velocity = Vector3.zero
        end)
        local TweenService = game:GetService("TweenService")
        local tweenInfo = TweenInfo.new(0.18, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut)
        local tween = TweenService:Create(humanoidrootpart, tweenInfo, {CFrame = humanoidrootpart.CFrame * CFrame.Angles(math.rad(0), 0, 0)})
        tween:Play()
        tween.Completed:Wait()
        task.wait(0.2)
        humanoidrootpart.CFrame = humanoidrootpart.CFrame * CFrame.Angles(math.rad(0), 0, 0)
        tweenInfo = TweenInfo.new(0.8, Enum.EasingStyle.Sine, Enum.EasingDirection.Out)
        tween = TweenService:Create(humanoidrootpart, tweenInfo, {CFrame = humanoidrootpart.CFrame * CFrame.Angles(math.rad(0), 0, 0)})
        tween:Play()
        tween.Completed:Wait()
		humanoidrootpart.CFrame = humanoidrootpart.CFrame * CFrame.Angles(math.rad(0), 0, 0)
        tweenInfo = TweenInfo.new(4, Enum.EasingStyle.Sine, Enum.EasingDirection.Out)
        tween = TweenService:Create(humanoidrootpart, tweenInfo, {CFrame = humanoidrootpart.CFrame * CFrame.Angles(math.rad(0), 0, 0)})
        tween:Play()
        end)
    ---------------------------------------------
    ---------------< general >-------------
        task.spawn(function()
        for _, track in pairs(humanoid:GetPlayingAnimationTracks()) do
            track:Stop()
        end
        local org_WalkSpeed = humanoid.WalkSpeed
        local org_AutoRotate = humanoid.AutoRotate
        local org_JumpPower = humanoid.JumpPower
        humanoid.WalkSpeed = 5
        humanoid.AutoRotate = false
        humanoid.JumpPower = 0
        task.wait(3)
        humanoid.WalkSpeed = org_WalkSpeed
        humanoid.AutoRotate = org_AutoRotate
        humanoid.JumpPower = org_JumpPower
        humanoid.HipHeight = 0
        active_move = nil
        end)
    ---------------------------------------
end
local hTwenty_SERIES_skill = Instance.new("Tool")
hTwenty_SERIES_skill.Name = "honored one"
hTwenty_SERIES_skill.RequiresHandle = false
hTwenty_SERIES_skill.Parent = player.Backpack
hTwenty_SERIES_skill.Equipped:Connect(function()
    hTwenty_SERIES()
    task.wait()
    hTwenty_SERIES_skill.Parent = player.Backpack
end)
